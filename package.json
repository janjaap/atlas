    "lodash.debounce": "^4.0.8",
    "lodash.defer": "^4.1.0",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.identity": "^3.0.0",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "marked": "^0.5.0",
    "marzipano": "https://github.com/Amsterdam/marzipano.git#master",
    "objectFitPolyfill": "^2.1.1",
    "proj4": "^2.4.4",
    "proj4leaflet": "^1.0.2",
    "prop-types": "^15.6.0",
    "raven-js": "^3.19.1",
    "react": "16.2.0",
    "react-dom": "^16.2.0",
    "react-leaflet": "^1.9.0",
    "react-leaflet-markercluster": "^1.1.8",
    "react-redux": "^5.0.7",
    "react-resize-aware": "^2.7.0",
    "recharts": "^1.2.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "remove-markdown": "^0.2.2",
    "reselect": "^3.0.1"
